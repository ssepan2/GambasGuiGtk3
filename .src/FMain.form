# Gambas Form File 3.0

{ Form Form
  MoveScaled(0,0,64,64)
  Icon = Picture["icon:/32/gnu"]
  { MenuFile Menu
    Text = Shortcut(("File"), "F")
    { MenuFileNew Menu
      Text = Shortcut(("New"), "N")
    }
    { MenuFileOpen Menu
      Text = Shortcut(("Open"), "O") & "..."
    }
    { MenuFileSave Menu
      Text = Shortcut(("Save"), "S")
    }
    { MenuFileSaveAs Menu
      Text = Shortcut(("Save As"), "a") & "..."
    }
    { Menu1 Menu
    }
    { MenuFileExit Menu
      Text = Shortcut(("Exit"), "x")
    }
  }
  { MenuHelp Menu
    Text = Shortcut(("Help"), "H")
    { MenuHelpAbout Menu
      Text = Shortcut(("About"), "A")
    }
  }
  { Label1 Label
    MoveScaled(6,42,24,4)
    Text = ("type something -->")
  }
  { TextBox1 TextBox
    MoveScaled(32,41,24,4)
  }
  { ButtonClose Button
    MoveScaled(45,50,16,4)
    Text = ("Close")
  }
  { Panel1 Panel
    MoveScaled(1,55,62,5)
    Border = Border.Sunken
    { StatusLabel Label
      MoveScaled(1,1,31,3)
    }
    { StatusPictureBox PictureBox
      MoveScaled(58,1,4,3)
      AutoResize = True
    }
    { StatusProgressBar ProgressBar
      MoveScaled(33,1,24,3)
    }
  }
  { ToolBar1 ToolBar
    MoveScaled(1,0,62,4)
    Separator = True
    { ToolButtonFileNew ToolButton
      MoveScaled(1,0,3,4)
      ToolTip = ("New")
      AutoResize = True
      Picture = Picture["icon:/22/new"]
    }
    { ToolButtonFileOpen ToolButton
      MoveScaled(4,0,4,4)
      ToolTip = ("Open")
      AutoResize = True
      Picture = Picture["icon:/22/open"]
    }
    { ToolButtonFileSave ToolButton
      MoveScaled(7,0,4,4)
      ToolTip = ("Save")
      AutoResize = True
      Picture = Picture["icon:/22/save"]
    }
    { ToolButtonFileSaveAs ToolButton
      MoveScaled(10,0,4,4)
      ToolTip = ("Save As") & "..."
      AutoResize = True
      Picture = Picture["icon:/22/save-as"]
    }
  }
}
